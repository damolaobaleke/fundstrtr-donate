                                                        <input class="form-control my-1" id="team_position" type="text" name="pitch[teamMember3.position]" placeholder="position" value="<%=data.teamMember3.name%>">
                                                        <textarea class="form-control my-1" id="team_details" type="text" name="pitch[teamMember3.details]" placeholder="Details" cols="30" rows="8" maxlength="300"><%=data.teamMember3.details%></textarea>
                                                    </div>
                                                </div>
