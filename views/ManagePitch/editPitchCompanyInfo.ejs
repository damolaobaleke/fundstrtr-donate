                                                        <input class="form-control my-1" id="team_name" type="text" name="pitch[teamMember2.name]" placeholder="name" value="<%=data.teamMember2.name%>" required>
                                                        <input class="form-control my-1" id="team_position" type="text" name="pitch[teamMember2.position]" placeholder="position" value="<%=data.teamMember2.position%>" required>
                                                        <textarea class="form-control my-1" id="team_details" type="text" name="pitch[teamMember2.details]" placeholder="Details" cols="30" rows="8" maxlength="300" required><%=data.teamMember2.details%></textarea>
                                                    </div>
